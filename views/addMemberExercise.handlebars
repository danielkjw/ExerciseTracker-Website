
<form id='addMemberExercise11' action='/memberexercise/id' method='post'> 
<div class="form-group">
<label class="mr-sm-2" for="exercise_id">Exercise List:</label>
  <select class="custom-select mb-2 mr-sm-2 mb-sm-0" name="exercise_id" id="exercise_id">
    <option selected value="{{exercise.[0].eid}}">{{exercise.[0].ename}}</option>
    {{#each exercise}}
    <option value="{{eid}}">{{ename}}</option>
    {{/each}}
  </select>
</div>
</form>
<button type="submit" value="Submit" onclick="addMemberExercise({{member.[0].mid}})"class="btn btn-primary" data-id="{{meid}}">Add</button>
